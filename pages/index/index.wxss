/**index.wxss**/
.search{
  width: 100%;
  height: 100rpx;
  background: #ff2d4a;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 12px;
  color: #ccc;
}
.searchson{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  background: #fff;
  text-align: center;
  border-radius: 4px;
}
.searchson icon{
  margin-right: 5px;
  height: 100%;
  padding-top: 2px;
  vertical-align: middle;
}
.swiper,.swiper image{
  width: 100%;
  height: 340rpx;
}